{% for result in results %}
<hr>
  {{ 'name: ' ~ result.name }}<br />
  {{ 'address: ' ~ result.address }}<br />
  {{ 'phone_number: ' ~ result.phoneNumber }}<br />
  {{ 'company: ' ~ result.company }}<br />
  {{ 'email: ' ~ result.email }}<br />
  {{ 'url: ' ~ result.url }}<br />
  {{ 'datetime: ' ~ result.datetime|date}}<br />
  {{ 'date: ' ~ result.date|date }}<br />
  {{ 'time: ' ~ result.time|date }}<br />
  {{ 'text: ' ~ result.text }}<br />
  {{ 'path: ' ~ result.path }}<br />
  {{ 'mimeType: ' ~ result.mimeType }}<br />
  {{ 'size: ' ~ result.size }}<br />
{% endfor %}